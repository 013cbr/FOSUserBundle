{% form_theme form 'FOSUserBundle::form.html.twig' %}
<form action="{{ path('fos_user_user_change_password_update') }}" {{ form_enctype(form) }} method="POST" class="fos_user_user_change_password">
    {{ form_widget(form) }}
    <div>
        <input type="submit" value="{% trans from 'FOSUserBundle' %}user.change_password.submit{% endtrans %}" />
    </div>
</form>
